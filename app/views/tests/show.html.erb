<p>
  <strong>Title:</strong>
  <%= @test.title %>
</p>

<p>
  <strong>Level:</strong>
  <%= @test.level %>
</p>

<table>
  <tr>
    <th>Question</th>
    <th>Action</th>
  </tr>

  <% @test.questions.each do |question| %>
    <tr>
      <td><%= question.body %></td>
      <td><%= 'action' %></td>
    </tr>
  <% end %>
</table>

<%= link_to 'Edit', edit_test_path(@test) %> |
<%= link_to 'Back', tests_path %>
